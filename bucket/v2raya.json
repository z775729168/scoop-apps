{
    "version": "1.5.6.2",
    "description": "v2rayA is a V2Ray client supporting global transparent proxy(Linux Only), compatible with SS, SSR, Trojan(trojan-go), PingTunnel protocols.",
    "homepage": "https://github.com/v2rayA/v2raya",
    "license": "AGPL-3.0-only",
    "architecture": {
        "64bit": {
            "url": "https://github.com/v2rayA/v2rayA/releases/download/v1.5.6.2/v2raya_windows_x64_1.5.6.2.exe",
            "hash": "34E3653190A730A6C20A54F16C48913357C81BCD11383F33CBFDBD30E03D1208"
        }
    },
    "installer": {
        "script": [
            "Rename-Item -Path $dir\\v2raya_windows_x64_$version.exe -NewName v2rayaWin.exe -ErrorAction Ignore"
        ]
    },
    "bin": [
        [
            "v2rayaWin.exe",
            "v2raya",
            "--lite"
        ],
        [
            "v2rayaWin.exe",
            "v2rayaWin"
        ]
    ],
    "depends": "v2ray",
    "suggest": "v2raya/v2ray-rules-dat",
    "env_set": {
        "V2RAYA_V2RAY_BIN": "$scoopdir\\apps\\v2ray\\current\\v2ray.exe"
    },
    "notes": [
        "Data will be saved to $env:HOME\\.config\\v2raya\\",
        "WARNING: ",
        "The database has been replaced with boltdb to try to solve the problem of database corruption. #267",
        "Note that the data will not be migrated, but the previous data will be retained. If you need the previous data, just downgrade v2rayA (v1.5.4)."
    ]
}
