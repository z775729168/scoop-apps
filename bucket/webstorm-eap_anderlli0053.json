    "hash": "70e9fe164c914bc10d36a5f0a70c2252aaaa50c89e3ece04c2a5852d65ee6c67",
    "extract_to": "IDE",
    "installer": {
        "script": [
            "& \"$bucketsdir\\versions\\scripts\\jetbrains\\portable.ps1\" $dir $persist_dir",
            "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse"
        ]
    },
    "architecture": {
        "64bit": {
            "bin": [
                [
                    "IDE\\bin\\webstorm64.exe",
                    "webstorm"
