 "pre_install": [
  "$bin_name = if ($architecture -eq '64bit') {'amd64'} else {'386'}",
  "Rename-Item \"$dir\\bin\\swctl-$version-windows-$bin_name\" \"swctl.exe\"",
  "Remove-Item \"$dir\\bin\\swctl-*\"",
  "Get-ChildItem -Path \"$dir\" \"._*\" -Recurse | Remove-Item"
 ],
