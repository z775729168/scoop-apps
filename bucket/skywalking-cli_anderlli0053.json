 },
 "bin": "bin\\swctl.exe",
 "checkver": {
  "regex": "(?sm)<a\\s+href\\=\\\"(?:[\\d.]+)\\/\\\">\\s*([\\d.]+)\\/?<\\/a>",
  "url": "https://downloads.apache.org/skywalking/cli/"
 },
 "description": "SkyWalking CLI is a command interaction tool for the SkyWalking user or OPS team",
 "extract_dir": "skywalking-cli-0.11.0-bin",
