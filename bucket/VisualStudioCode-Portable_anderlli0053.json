  "  $content = $content.Replace('$code', $codepath)",
  "  if ($global) {",
  "    $content = $content.Replace('HKEY_CURRENT_USER', 'HKEY_LOCAL_MACHINE')",
  "  }",
  "  $content | Set-Content -Path \"$dir\\vscode-install-context.reg\"",
  "}",
