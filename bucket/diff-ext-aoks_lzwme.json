    "args": [
      "/VERYSILENT",
      "/DIR=\"$dir\"",
      "/SUPPRESSMSGBOXES",
      "/NORESTART",
      "/CLOSEAPPLICATIONS",
      "/NORESTARTAPPLICATIONS"
    ],
    "file": "diff-ext.exe"
  },
  "license": "BSD-2-Clause",
  "shortcuts": [
    [
      "diff-ext-setup.exe",
      "diff-ext Setup"
    ]
  ],
  "uninstaller": {
    "args": [
      "/VERYSILENT",
      "/SUPPRESSMSGBOXES",
      "/NORESTART"
    ],
    "file": "unins000.exe"
  },
