{
  "version": "4.13.2",
  "description": "Modern source port for Doom, Heretic, Hexen and more",
  "homepage": "https://zdoom.org/",
  "license": "GPL-3.0-or-later",
  "notes": [
    "Place WAD files (game data) in:",
    "",
    "    $persist_dir\\..\\_doom"
  ],
  "architecture": {
    "64bit": {
      "url": "https://mirror.ghproxy.com/https://github.com/ZDoom/gzdoom/releases/download/g4.13.2/gzdoom-4-13-2-Windows.zip",
      "hash": "5ac5d133f8f82c1c2be8918bfafb2adf82faf76a1869caddc0251c49a0804f4c"
    }
  },
  "pre_install": [
    "New-Item -ItemType Directory -Force -Path $persist_dir\\..\\_doom | Out-Null",
    "New-Item -Path $dir -Name gzdoom_portable.ini -ItemType File -ErrorAction Ignore | Out-Null"
  ],
  "env_set": {
    "DOOMWADDIR": "$persist_dir\\..\\_doom"
  },
  "bin": "gzdoom.exe",
  "shortcuts": [
    [
      "gzdoom.exe",
      "GZDoom"
    ]
  ],
  "persist": [
    "gzdoom_portable.ini",
    "Save",
    "Screenshots"
  ],
  "checkver": {
    "github": "https://github.com/ZDoom/gzdoom",
    "regex": "/releases/tag/(?:g)?([\\w.]+)"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "https://mirror.ghproxy.com/https://github.com/ZDoom/gzdoom/releases/download/g$version/gzdoom-$dashVersion-Windows.zip"
      }
    }
  }
}