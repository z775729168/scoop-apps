  "dotnet-runtime": "meta/dotnet-runtime"
 },
 "uninstaller": {
  "script": [
   "'config.yaml', 'guiNConfig.json' | ForEach-Object {",
   "    if (Test-Path \"$dir\\$_\") { Copy-Item \"$dir\\$_\" \"$persist_dir\\\" -Force | Out-Null }",
   "}"
  ]
 },
