    },
    "url": "https://download.jetbrains.com/python/pycharm-professional-221.6008.5.exe#/dl.7z",
    "hash": "a4acdcc9dc0f0a13e297c468e6317673310246ea4857509e40639bc75fd58e4c",
    "extract_to": "IDE",
    "installer": {
        "script": [
            "& \"$bucketsdir\\versions\\scripts\\jetbrains\\portable.ps1\" $dir $persist_dir",
            "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse"
        ]
    },
    "architecture": {
        "64bit": {
            "bin": [
                [
                    "IDE\\bin\\pycharm64.exe",
                    "pycharm"
