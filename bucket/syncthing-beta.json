    },
    "notes": "To start syncthing automatically, use a method described at https://docs.syncthing.net/users/autostart.html#windows",
    "architecture": {
        "64bit": {
            "url": "https://github.com/syncthing/syncthing/releases/download/v1.27.11-rc.2/syncthing-windows-amd64-v1.27.11-rc.2.zip",
