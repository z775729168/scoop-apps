        [
            "MouseInc.exe",
            "MouseInc"
        ]
    ],
    "extract_dir": "MouseInc",
    "checkver": {
        "regex": "MouseInc ([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://update.shuax.com/MouseInc$version.7z"
    }
