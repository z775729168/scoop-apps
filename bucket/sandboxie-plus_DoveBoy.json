    "notes": "Get Sandboxie Classic if you have used Sandboxie for a long time and prefer a familiar environment over new features.",
    "architecture": {
        "64bit": {
            "url": "https://github.com/sandboxie-plus/Sandboxie/releases/download/v1.10.3/Sandboxie-Plus-x64-v1.10.3.exe",
            "hash": "a0f822404bcdb7abef06e57e43b444fb6d9356950e83ec7c621b3e1074682150"
        },
        "32bit": {
            "url": "https://github.com/sandboxie-plus/Sandboxie/releases/download/v1.10.3/Sandboxie-Plus-x86-v1.10.3.exe",
