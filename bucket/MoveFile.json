    },
    "notes": "Introduction: <https://docs.microsoft.com/en-us/sysinternals/downloads/pendmoves#introduction>",
    "url": "https://download.sysinternals.com/files/pendmoves.zip",
    "hash": "377c0a8efbf39a78add4a9fe421ae140400b1a0a1e7f7f447793c690001410a3",
    "pre_install": "Remove-Item \"$dir\\pendmoves*\" -Force -ErrorAction SilentlyContinue",
    "architecture": {
        "64bit": {
            "post_install": [
                "@('movefile.exe', 'movefile64a.exe') | ForEach-Object { Remove-Item \"$dir\\$_\" -Force -ErrorAction SilentlyContinue }",
                "Rename-Item \"$dir\\movefile64.exe\" 'movefile.exe' -Force -ErrorAction SilentlyContinue"
            ]
        },
        "32bit": {
            "post_install": "@('movefile64.exe', 'movefile64a.exe') | ForEach-Object { Remove-Item \"$dir\\$_\" -Force -ErrorAction SilentlyContinue }"
        }
    },
    "bin": "movefile.exe",
    "checkver": {
        "url": "https://docs.microsoft.com/en-us/sysinternals/downloads/pendmoves",
        "regex": "MoveFile v([\\d.]+)"
    },
