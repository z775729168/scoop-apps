    "bin": "bin\\quarto.cmd",
    "checkver": {
        "url": "https://quarto.org/docs/download/_prerelease.json",
        "jp": "$.version"
    },
    "autoupdate": {
        "url": "https://github.com/quarto-dev/quarto-cli/releases/download/v$version/quarto-$version-win.zip",
        "hash": {
            "url": "https://quarto.org/docs/download/_prerelease.json",
