        "replace": "${version}-${build}-g${commit}"
    },
    "autoupdate": {
        "architecture": {
            "32bit": {
                "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v$matchVersion-$matchBuild-g$matchCommit&platform=windows-x86#/dl.zip"
