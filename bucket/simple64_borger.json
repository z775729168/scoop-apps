        [
            "simple64-gui.exe",
            "simple64"
        ]
    ],
    "persist": "config",
    "checkver": {
        "url": "https://api.github.com/repos/simple64/simple64/releases/latest",
        "regex": "v([\\d\\.]+)/simple64-win64-(?<commit>[\\da-f]{7}).zip"
    },
    "autoupdate": {
