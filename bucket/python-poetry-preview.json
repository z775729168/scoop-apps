{
    "version": "1.4.0.dev0",
    "homepage": "https://python-poetry.org/",
    "description": "Python dependency management and packaging made easy.",
    "url": "https://raw.githubusercontent.com/python-poetry/poetry/master/install-poetry.py",
    "hash": "5c812616fb08ce8d72905ea24dd1a9be1f38717408377a8656efc4b5648c93cb",
    "license": "MIT",
    "depends": "python",
    "env_set": {
        "POETRY_HOME": "$dir"
    },
    "bin": "bin\\poetry.exe",
    "checkver": {
        "url": "https://raw.githubusercontent.com/python-poetry/poetry/master/pyproject.toml",
        "regex": "version = \"([\\d.\\-a-z]+)\""
    },
    "autoupdate": {
        "url": "https://raw.githubusercontent.com/python-poetry/poetry/master/install-poetry.py"
    },
    "installer": {
        "script": "$env:POETRY_HOME=$dir; python $dir/$fname --preview -y"
    },
    "uninstaller": {
        "script": "python $dir/install-poetry.py --uninstall -y"
    }
}
