  "post_install": [
    "if (!(Test-Path \"$env:USERPROFILE\\.profile\")) {",
    "    info 'creating ~/.profile'",
    "@\"",
    "if [ -e ~/.bashrc ]; then",
    "   source ~/.bashrc",
    "fi",
    "\"@ | Out-File \"$env:USERPROFILE\\.profile\" -Encoding oem",
    "}"
