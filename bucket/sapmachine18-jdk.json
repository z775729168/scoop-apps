    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/SAP/SapMachine/releases/download/$matchSap.zip"
            }
        },
        "hash": {
            "url": "https://github.com/SAP/SapMachine/releases/download/$matchSap.sha256.txt",
            "find": "^($sha256)\\s"
        },
        "extract_dir": "sapmachine-jdk-$matchVersion"
