    "pre_install": "Set-Content \"$dir\\sbscan.bat\" '@echo off\npushd %~dp0\ncall \"%~dp0\\sbscan_env\\Scripts\\activate.bat\"\npython3 \"%~dp0\\sbscan.py\" %*\npopd' -Encoding Ascii",
    "post_install": [
        "Move-Item -Path \"$dir\\SBSCAN-$version\\*\" -Destination \"$dir\\\"",
