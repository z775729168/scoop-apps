    "shortcuts": [
        [
            "balenaEtcher.exe",
            "Etcher"
        ]
    ],
    "checkver": {
        "url": "https://github.com/balena-io/etcher/releases.atom",
        "regex": "/releases/tag/v([\\d.]+)"
