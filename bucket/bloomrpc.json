{
    "version": "1.5.2",
    "description": "BloomRPC aims to provide the simplest and most efficient developer experience for exploring and querying your GRPC services.",
    "homepage": "https://github.com/bloomrpc/bloomrpc",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/bloomrpc/bloomrpc/releases/download/1.5.2/BloomRPC-Setup-1.5.2.exe#/dl.7z",
            "hash": "sha512:6748bf3fdfe78378a131681ab104b5dad9ca19834be120aee63107acebf5647858c3588f19e5c76b991b86319338e3def9f4c7a5f25849e7d01f0231c5ea47ad",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
                "Remove-Item \"$dir\\`$*\", \"$dir\\Uninst*\" -Force -Recurse"
            ]
        }
    },
    "shortcuts": [
        [
            "BloomRPC.exe",
            "BloomRPC"
        ]
    ],
